{
  "name": "@ngrx-utils/store",
  "version": "0.10.0",
  "description": "Utilities to help reduce your code boilerplate when working with @ngrx/store",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngrx-utils/ngrx-utils.git"
  },
  "keywords": [
    "@ngrx/store",
    "NgRx",
    "Angular",
    "Store",
    "Redux",
    "@ngrx-utils/store"
  ],
  "author": "San Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngrx-utils/ngrx-utils/issues"
  },
  "homepage": "https://github.com/ngrx-utils/ngrx-utils#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@angular/core": "^5.1.3",
    "@angular/router": "^5.1.3",
    "@ngrx/store": "^5.0.0",
    "rxjs": "^5.5.6"
  },
  "ngPackage": {
    "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
    "workingDirectory": "../../.ng_build",
    "dest": "../../dist/store",
    "lib": {
      "entryFile": "public_api.ts",
      "umdModuleIds": {
        "@ngrx/store": "ngrx.store",
        "@ngrx-utils/store": "ngrxUtils.store"
      }
    }
  }
}
